{
	"name": "ticode",
	"publisher": "HenryHeDiff",
	"icon": "https://github.com/HenryC-3/vscode.plugin.ticode/blob/master/icon.png",
	"displayName": "ticode",
	"description": "ticode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ticode.addTask",
		"onCommand:ticode.generateURL"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ticode.addTask",
				"title": "ticode: add current page to ticktick"
			},
			{
				"command": "ticode.generateURL",
				"title": "ticode: generate vscode url scheme for current file"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
